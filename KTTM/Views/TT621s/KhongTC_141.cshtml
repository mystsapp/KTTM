@section Scripts{
    @*<script src="~/js/Admin/Home/indexController.js"></script>*@
    <!--mustache-->
    <script src="~/lib/mustache.min.js"></script>
    @*<script src="~/js/Admin/TT621/createController.js"></script>*@
    <script src="~/js/Admin/TT621/khongTCController.js"></script>

    <script id="data-template" type="x-tmpl-mustache">

        <tr class="trTT621" data-id="{{Id}}" data-phieutc="{{PhieuTC}}">

            <td>{{SoCT}}</td>
            <td>{{DienGiai}}</td>
            <td>{{DienGiaiP}}</td>
            <td>{{SoTienNT}}</td>
            <td>{{LoaiTien}}</td>
            <td>{{TyGia}}</td>
            <td>{{SoTien}}</td>
            <td>{{TKNo}}</td>
            <td>{{MaKhNo}}</td>
            <td>{{TKCo}}</td>
            <td>{{MaKhCo}}</td>
            <td>{{Sgtcode}}</td>

        </tr>
    </script>

    <script>

        var id_Redirect = '@ViewBag.tamUngId'; //Id sau khi redirect ve //////////////////////////////////////////////
        if (id_Redirect !== '') {
            khongTCController.Disabled_TU_Khong_TT(id_Redirect); // Disabled row TU ko co TT
            $('.' + id_Redirect).click();
        }

        function confirmDelete(uniqueId, isDeleteClicked) {
            var deleteSpan = 'deleteSpan_' + uniqueId;
            var confirmDeleteSpan = 'confirmDeleteSpan_' + uniqueId;

            if (isDeleteClicked) {
                $('#' + deleteSpan).hide();
                $('#' + confirmDeleteSpan).show();
            } else {
                $('#' + deleteSpan).show();
                $('#' + confirmDeleteSpan).hide();
            }
        }

        // contextmenu for themmoiTT VND btnThemMoiCT
        $('#btnThemMoiCT').contextmenu(function (e) {
            e.preventDefault();

            tamUngId = $('#hidTamUngId').val();
            var loaiTien = '';
            $.get('/TT621s/GetLoaiTien', { tamUngId: tamUngId }, function (response) {
                
                loaiTien = response;
                
                if (loaiTien === 'VND') {
                    tamUngId = $('#hidTamUngId').val();

                    if (tamUngId == null || tamUngId == '') {
                        alert('chưa chọn tạm ứng nào để thanh toán');
                    }
                    $('#tamUngId_ContextMenu').val(tamUngId);

                    id_Dong_Da_Click = $('#hidIdCu').val();
                    $('#id_Dong_Da_Click_ContextMenu').val(id_Dong_Da_Click);

                    //kVCTPCTId_PhieuTC = $('#hidKVCTPCTId').val(); // dung de: GetDummyTT621_By_KVCTPCT. TT621Create view
                    //$('#kVCTPCTId_PhieuTC').val(kVCTPCTId_PhieuTC);

                    $('#frm_ThemMoiCT_TT_ContextMenu_KhongTC').submit();
                }
                else {
                    toastr.warning("Hãy chọn phiếu TT VN.");
                    return;
                }

            })
            
        });
        // importexcel

        $('#importExcel_Submit').click(function () {
            var tamUngId = $('#hidTamUngId').val();
            if (tamUngId === '') {
                alert('Bạn chưa chọn TU nào.');
                return false;
            }
            var loaiPhieu = '@Model.KVPTC.MFieu';
            var kvptcId = '@Model.KVPTC.Id';
            var kvctptcId = '@Model.KVCTPTC.Id';

            var fileExtension = ['xls', 'xlsx'];
            var filename = $('#fUpload').val();
            if (filename.length !== 0) {
                var extension = filename.replace(/^.*\./, '');
                // kiem tra dinh dang
                if ($.inArray(extension, fileExtension) === -1) {
                    bootbox.alert({
                        size: "small",
                        title: "Infomation",
                        message: "Chọn chưa đúng định dạng <b> Excel! </b>"
                    });

                    return false;
                }
            }

            if ($('#frmImportExcell').valid()) {

                var input = $('#fUpload');
                var files = input[0].files;
                if (files.length == 0) {
                    toastr.warning("Vui lòng chọn file excel.");
                    return;
                }

                $('#importExcel_Submit').attr('disabled', true);
                $('.submitSpin').show();

                var formData = new FormData();
                formData.append("tamUngId", tamUngId);
                formData.append("loaiPhieu", loaiPhieu);
                formData.append("kvptcId", kvptcId);
                formData.append("kvctptcId", kvctptcId);

                for (var i = 0; i <= files.length; i++) {
                    formData.append("files", files[i]);
                }
                $.ajax({
                    type: "POST",
                    url: "/TT621s/ImportExcell",
                    data: formData,
                    processData: false,
                    contentType: false,
                    success: function (response) {

                        if (response.status) {
                            toastr.success('Import data thành công!');
                            $('#importExcelModal').modal('hide');
                            //// load lai TT621Create
                            $('#' + tamUngId).click();
                            khongTCController.Disabled_TU_Khong_TT(tamUngId); // Disabled row TU ko co TT

                        } else {
                            $('#importExcelModal').modal('hide');
                            toastr.error(response.message);

                        }
                    }
                    , error: function (xhr, status, error) {
                        $('.modal').modal('hide');
                        alert("Có lỗi: " + error + " ,xin thông báo cho người quản lý biết!");
                    }
                });
            }
            else {
                alert('Not valid')
            }
        })

        // btnKhSearch
        $('#btnKhSearch').click(function () { // khi click có the dinh' nut txtMaKh blur (su kien blur khi go~ dang do~ va bam' nut btnKhSearch)

            var maKhtext = $('#txtMaKh').val();
            var kvpctId = '';
            if ('@Model.KVPTC.SoCT' != null) {
                kvpctId = '@Model.KVPTC.SoCT';
            }
            var strUrl = '@Model.StrUrl';
            var page = '@Model.Page';

            KhachHangSearch_KhongTC(maKhtext, kvpctId, strUrl, page);
        })

        $('#txtMaKh').blur(function () {

            code = $(this).val();

            $.ajax({
                url: '/TT621s/GetKhachHangs_By_Code_KhongTC',
                type: 'GET',
                data: { code: code },
                dataType: 'json',
                success: function (response) {
                    if (response.status) {
                        var data = response.data;
                        // console.log(data);

                        //$('#txtMaKh').val(data.code);
                        //$('#txtTenKh').val(data.name);

                        // submit qua KhongTC_141 <-> frm_KhongTC_141
                        $('#hidKvpctId').val('@Model.KVPTC.SoCT');
                        $('#hidStrUrl').val('@Model.StrUrl');
                        $('#hidPage').val('@Model.Page');
                        $('#hidMaKh').val(data.code);
                        $('#hidTenKh').val(data.name);

                        $('#frm_KhongTC_141').submit();
                    }
                    else {// search ko co KH nao het => ...

                        //$('#txtMaKh').val('');
                        $('#txtTenKh').val('');
                    }
                }
            });

