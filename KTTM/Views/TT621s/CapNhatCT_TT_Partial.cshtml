<script>

    // enter for search
    shortcut.add("Enter", function () {
        var btnKhSearch_CapNhat_Status = $('#btnKhSearch_CapNhat').hasClass('disabled');
        if (!btnKhSearch_CapNhat_Status) {
            $('#btnKhSearch_CapNhat').click()
        }
        
    });

    // btnKhSearch
    $('#btnKhSearch_CapNhat').click(function () {

        var maKhtext = '';

        if ($(this).data('name') === 'maKhNo') { // search of no
            maKhtext = $('#txtMaKhNo_CapNhat').val();
        }
        if ($(this).data('name') === 'maKhCo') { // search of co
            maKhtext = $('#txtMaKhCo_CapNhat').val();
        }

        KhachHangSearch(maKhtext);
    })

    $('#txtMaKhNo_CapNhat').blur(function () {
        
            maKhtext = $(this).val();

            //KhachHang_By_Code(maKhtext);
            createController.KhachHang_By_Code_CapNhat(maKhtext, 'txtMaKhNo');
        })

    $('#txtMaKhCo_CapNhat').blur(function () {

            var tkCo = '@Model.KVCTPTC.TKCo';
            if (tkCo !== "1411") {
                maKhtext = $(this).val();

                indexController.KhachHang_By_Code(maKhtext, 'txtMaKhCo');
            }
        })

        $('#ddlNgoaiTe').change(function () {
            loaiTien = $(this).val();
            loaiPhieu = '@Model.KVCTPTC.KVPTC.MFieu';
            serviceController.DdlLoaiTien(loaiTien, loaiPhieu);
        })

    $('#frm_CapNhatCT_TT_Partial').validate({
        //rules: {
